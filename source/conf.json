{
  "plugins": ["plugins/markdown"],
  "source": {
      "include": ["./assets/scripts/"]
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "opts": {
    "destination": "./docs" 
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}